<Window x:Class="PL.TimeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="TimeWindow" Height="450" Width="800">
    <Grid>
        <Label Content="Enter Travel Time From The Previous Station :" HorizontalAlignment="Left" Margin="156,48,0,0" VerticalAlignment="Top" Height="56" Width="426" FontSize="32" FontFamily="Niagara Engraved"/>
        <Button Content="CONFIRM ADD TIME" HorizontalAlignment="Left" Margin="580,255,0,0" VerticalAlignment="Top" Width="170" Height="63" Click="Button_Click" RenderTransformOrigin="0.195,1.595" Background="#FFCEEB0D" FontFamily="Imprint MT Shadow"/>
        <TextBox x:Name="EnterTimeTb" HorizontalAlignment="Left" Height="63" Margin="198,216,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="194"/>

    </Grid>
</Window>
